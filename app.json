{
  "expo": {
    "name": "todolistapp",
    "slug": "todolistapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Aplikacja wymaga dostępu do galerii, aby wybrać obraz.",
        "NSCalendarsUsageDescription": "Aplikacja wymaga dostępu do kalendarza, aby eksportować zadania."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ],
      "package": "com.mwalas.todolistapp"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "todolistmobileapp",
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "00f8afa0-5e78-40e2-87b4-0104544fedd9"
      }
    },
    "owner": "mwalas"
  }
}
